
config_$alfred_name = PhYAMLConfig(self.job_path, self.$alfred_name)
config_$alfred_name.load_yaml()
cb_$alfred_name = ""
entry_point = config.spec.containers.code
if "/" not in entry_point:
    entry_point = self.job_path + "/" + entry_point
    cb = ["$alfred_command", entry_point]
    for arg in config.spec.containers.args:
        cb.append("--" + arg.key + "=" + str(arg.value))
    cb_$alfred_name = cb

$alfred_name = create_dag_job(cb_$alfred_name)
