config_$alfred_name = PhYAMLConfig(dags_path + "/", "$alfred_name" + ".yaml")
config_$alfred_name.load_yaml()
op_kwargs = {}
for arg in config_$alfred_name.spec.containers.args:
    op_kwargs[arg.key] = str(arg.value)

$alfred_name = PythonOperator(
                    task_id="$alfred_name",
                    python_callable=$alfred_name,
                    op_kwargs=op_kwargs,
                    dag=dag
               )
